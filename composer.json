{
    "name": "yuuan/readonly",
    "description": "Trait to define read-only properties",
    "type": "library",
    "homepage": "https://github.com/yuuan/readonly",
    "authors": [
        {
            "name": "yuuAn",
            "homepage": "https://github.com/yuuan"
        }
    ],
    "require": {
        "php": ">=7.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^0.12.81",
        "friendsofphp/php-cs-fixer": "^2.18"
    },
    "scripts": {
        "fix": "php-cs-fixer fix",
        "lint": "php-cs-fixer fix --dry-run",
        "phpstan": "phpstan analyse"
    },
    "autoload": {
        "psr-4": {
            "Yuuan\\ReadOnly\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yuuan\\Tests\\ReadOnly\\": "tests/"
        }
    },
    "license": "MIT"
}
